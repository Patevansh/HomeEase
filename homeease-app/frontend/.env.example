# Frontend Environment Configuration Example
# Copy this file to .env and update the values

# API Base URL - Replace with your deployed backend URL (without /api)
REACT_APP_API_URL=https://your-backend-api.vercel.app

# OAuth URLs - Replace with your deployed backend URL
REACT_APP_GOOGLE_AUTH_URL=https://your-backend-api.vercel.app/api/auth/google
REACT_APP_FACEBOOK_AUTH_URL=https://your-backend-api.vercel.app/api/auth/facebook

# App Configuration
REACT_APP_APP_NAME=HomeEase
REACT_APP_APP_VERSION=1.0.0

# Optional: Analytics and Tracking
# REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# REACT_APP_FACEBOOK_PIXEL_ID=xxxxxxxxxx

# Optional: Error Tracking
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn
